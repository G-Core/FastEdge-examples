{
  "name": "react-app",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "craco start",
    "build": "npm-run-all -p build:client build:server -s build:fastedge",
    "build:client": "craco build",
    "build:fastedge": "fastedge-build -c",
    "build:server": "node esbuild/server.js",
    "build:server:fe": "fastedge-build ./dist/server.js ./dist/server.wasm",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "itty-router": "^5.0.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.7.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.21.6",
    "@craco/craco": "^7.1.0",
    "@gcoredev/fastedge-sdk-js": "^1.1.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "esbuild": "^0.24.0",
    "eslint": "^8.18.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4",
    "web-vitals": "^4.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
